{
  "table": [
  {
    "name": "history",
    "use": "timebase",
    "title": "My measurements over time",
    "subtitle": "candy5",
    "attr" : "right",
    "digs" : 1,
    "select": [
        {
          "name": "timestamp", 
          "min" : 121000,
          "max" : 173000,
          "extra" : 0
        },
        {
          "name": "input", 
          "isoneof": [1,4,55]
        }
    ],
    "page" : "day",
    "average" : "none",
    
    "columns": [
        {
          "name": "timestamp", 
          "group": "columnhdr",
          "type" : "long",
          "attr": "time,char,left,w:10",
        },
        {
          "name": "input", 
          "type" : "int",
          "attr": "char",
        },
        {
          "name": "temperature", 
          "value" : "ok",
          "type" : "str",
          "attr": "left",
        },
        {
          "name": "humidity", 
          "status" : "ok",
          "type" : "float",
          "digs" : 3,
          "group": "Watery Stuff"
        },
        {
          "name": "water", 
          "unit": "mg/m3", 
          "attr": "char",
          "group": "Watery Stuff"
        }
    ],
    
    "rows": [
